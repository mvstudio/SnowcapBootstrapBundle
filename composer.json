    "target-dir": "Snowcap/BootstrapBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/phpunit-bridge": "^2.7",
        "doctrine/annotations": "^1.2"
