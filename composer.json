    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "^2.7",
        "leafo/lessphp": "v0.4.0",
        "twig/twig": "^1.22",
        "symfony/form": "^2.7"
    },
    "autoload": {
        "psr-0": { "Snowcap\\BootstrapBundle": "" }
